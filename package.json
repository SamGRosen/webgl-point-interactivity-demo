{
  "name": "epiviz.gl",
  "version": "1.0.0",
  "main": "dist/index.js",
  "app": "dist-app/index.html",
  "type": "module",
  "repository": "https://github.com/epiviz/epiviz.gl",
  "homepage": "https://github.com/epiviz/epiviz.gl",
  "author": "Sam Rosen",
  "license": "MIT",
  "targets": {
    "main": {
      "context": "node",
      "includeNodeModules": true,
      "isLibrary": true,
      "optimize": false,
      "outputFormat": "esmodule"
    },
    "app": {
      "context": "browser",
      "publicUrl": "./",
      "optimize": false
    }
  },
  "scripts": {
    "start": "parcel serve app/index.html --open",
    "build": "rm -rf dist-app/ && parcel build app/index.html --target app",
    "build-package": "rm -rf dist/ && parcel build src/index.js --target main && cp src/index.d.ts dist/index.d.ts",
    "predeploy": "yarn build",
    "deploy": "gh-pages -d dist-app",
    "copy": "source .env && rsync -qazP $DEPLOY_FILES $DEPLOY_TARGET",
    "cypress:open": "cypress open"
  },
  "dependencies": {
    "@turf/boolean-point-in-polygon": "^6.3.0",
    "@turf/helpers": "^6.3.0",
    "@turf/simplify": "6.3.0",
    "axios": "0.21.1",
    "d3-axis": "^3.0.0",
    "d3-color": "^3.0.1",
    "d3-format": "3.0.1",
    "d3-scale": "^3.0.0",
    "d3-scale-chromatic": "^3.0.0",
    "d3-selection": "^3.0.0",
    "fpsmeter": "0.3.1",
    "jsonschema": "1.4.0",
    "supercluster": "^7.1.3",
    "twgl.js": "^4.19.1"
  },
  "devDependencies": {
    "@reduxjs/toolkit": "^1.6.0",
    "cypress": "^7.6.0",
    "gh-pages": "^3.2.0",
    "parcel": "2.0.0-beta.2"
  }
}
